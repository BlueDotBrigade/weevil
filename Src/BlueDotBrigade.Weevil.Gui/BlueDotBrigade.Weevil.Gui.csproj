    </PackageReference>
    <PackageReference Include="NLog">
      <Version>4.7.15</Version>
    </PackageReference>
    <PackageReference Include="PostSharp">
      <Version>6.10.10</Version>
    </PackageReference>
    <PackageReference Include="PostSharp.Patterns.Model">
